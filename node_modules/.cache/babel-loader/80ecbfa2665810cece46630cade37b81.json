IAAf,GAAoB,KAAKZ,UAAzB;AACA,WAAKP,QAAL,GAAc,IAAd;AACAR,WAAK,CAAC4B,IAAN,CAAW,yDAAuD,KAAKtB,MAA5D,GAAmE,WAAnE,GAA+E,KAAKC,OAA/F,EAAwG,KAAKS,SAA7G,EAAwHa,IAAxH,CAA6H,kBAAQ,EAAG;AACjIC,eAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBC,QAAQ,CAAC3B,IAAhC;AACA,aAAI,CAACY,aAAL,GAAmBe,QAAQ,CAAC3B,IAA5B;AACA,aAAI,CAACG,QAAL,GAAc,KAAd;AAEC,OALR;AAMD,KArBM;AAuBPyB,YAvBO,oBAuBEN,IAvBF,EAuBO;AAAA;;AACX,WAAKlB,OAAL,GAAa,IAAb;AACKT,WAAK,CAACkC,GAAN,CAAU,sDAAoD,KAAK5B,MAAzD,GAAgE,WAAhE,GAA4E,KAAKC,OAAjF,GAA2F,+BAA3F,GAA2HoB,IAArI,EAA2IE,IAA3I,CAAgJ,kBAAQ,EAAG;AACvJ,cAAI,CAACX,YAAL,GAAkBc,QAAQ,CAAC3B,IAA3B;AACA,cAAI,CAACI,OAAL,GAAa,KAAb;AACH,OAHD,EAGG0B,KAHH,CAGS,aAAG,EAAG;AACX,cAAI,CAAC1B,OAAL,GAAa,KAAb;AACA,cAAI,CAACC,GAAL,GAAS,yCAAT;AACAoB,eAAO,CAACC,GAAR,CAAY,OAAZ,EAAoBrB,GAApB;AACH,OAPD;AASP;AAlCM;AA1BI,CAAf;AAmEA;;;AAGA","names":["Axios","useValidate","required","minValue","setup","data","app_id","app_key","loading1","loading","err","qty","unit","food","searchItem","searchList","post_data","post_response","get_response","methods","add","push","deleteItem","foodItem","filter","value","submit","ingr","post","then","console","log","response","getFacts","get","catch"],"sourceRoot":"","sources":["C:\\Users\\AGEORGEV\\nutrition-app\\src\\components\\Nutrition.vue"],"sourcesContent":["<template>\n<head>\n  <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\n  <link rel=\"stylesheet\" href=\"https://cdn.jsdelivr.net/npm/bootstrap@4.2.1/dist/css/bootstrap.min.css\" integrity=\"sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS\" crossorigin=\"anonymous\">\n</head> \n \n <h2>\n   Recipe Analyzer\n </h2>\n <br>\n\n  <div >\n  <div class= \"form-group\" style=\"display:flex\">\n    <label style=\"margin-left:40px\" for=\"qty\">Quantity</label>\n    <input type=\"number\" id=qty class=\"inputs\" v-model=\"qty\">\n    <br>\n\n    <label for=\"unit\">Unit</label> \n    <select class=\"inputs\" name=\"\" id=\"unit\" v-model=\"unit\">\n      <option value=\"cup\">Cup</option>\n      <option value=\"ounce\">Ounce (oz)</option>\n      <option value=\"pinch\">Pinch</option>\n      <option value=\"tablespoon\">Tablespoon</option>\n      <option value=\"teaspoon\">Teaspoon</option>\n      <option value=\"pint\">Pint</option>\n    </select>\n    <br>\n\n    <label for=\"food\">Food item</label>\n    <input type=\"text\" id=\"food\" class=\"inputs\" v-model=\"food\"> \n\n    <button class=\"btn-warning\" @click=\"add\" style=\"width:75px\"> Add </button>\n\n  </div>\n \n <div class=\"\">\n  <ol >\n    <li v-for=\"s in searchList\" :key=\"s\"> <span>{{s}}</span>  <button  @click=\"deleteItem(s)\"><i class=\"fa fa-trash\"></i></button></li>\n  </ol>\n  </div>\n\n  <button  id=\"pulser\" value=\"Submit\" @click=\"submit\">Submit</button>\n\n  </div>\n  <br>\n  <div v-if=\"loading1===true\">\n        <div class=\"spinner-border text-success\" role=\"status\">\n        <span class=\"sr-only\">Loading...</span>\n        </div>\n      </div>\n\n  <div class=\"container\" v-else-if=\"Object.keys(post_response).length!==0\">\n    \n    <b >Calories: {{post_response.calories}}</b> \n    <br>\n    <b > <ul>\n      <li v-for=\"l in post_response.dietLabels\" :key=\"l\">\n        {{l}}\n      </li> \n      </ul></b>\n\n      <b>Weight : {{post_response.totalWeight}} g</b>\n  </div>\n  <br>\n   <br>\n   <br>\n  <p>Nutritional Facts about common food items: </p>\n  <br>\n\n  <div class=\"container\">\n\n  <table class=\"centred\">\n    <th>Food Item List</th>\n    <th rowspan=\"4\" class=\"panel\">\n      <p v-if=\"Object.keys(get_response).length===0\" style=\"color:darkslateblue\">\n         Select a food item from the list to see its analysis\n      </p>\n\n      <div v-else-if=\"loading===true\">\n        <div class=\"spinner-border text-danger\" role=\"status\">\n        <span class=\"sr-only\">Loading...</span>\n        </div>\n      </div>\n\n      <ul v-else-if=\"Object.keys(get_response).length!==0\">\n      <li>Calories: {{get_response.calories}}</li>\n      <li> Weight: {{get_response.totalWeight}} g</li>\n      <li v-for=\"l in get_response.dietLabels\" :key=\"l\">\n        {{l}}\n      </li> \n      \n    </ul>\n    </th>\n    <tr>\n       <div class=\"card\" @click=\"getFacts('1 tomato')\">\n        <p ><em> Tomato</em></p>\n       </div>\n    </tr> \n\n    <tr>\n       <div class=\"card\" @click=\"getFacts('1 Egg')\">\n    <p ><em> Egg</em></p>\n      </div>\n    </tr>\n\n    <tr>\n       <div class=\"card\" @click=\"getFacts('1 Potato')\">\n       <p ><em> Potato</em></p>\n       </div>\n    </tr>\n  </table>\n\n  </div>\n\n</template>\n\n<script> \n//api-id =4cc00ec3\n//api-key=3ffd1eb7d2245ac2cf44739c395c9ff0, 862787ee84da281d5b0b31e3a70df086\t\nimport Axios from 'axios'\nimport useValidate from '@vuelidate/core'\nimport { required, minValue } from 'vuelidate/lib/validators'\n\nexport default {\n  setup () {\n    return {\n    }\n  },\n\n  data () {\n    return {\n      app_id:\"4cc00ec3\",\n      app_key:'3ffd1eb7d2245ac2cf44739c395c9ff0',\n      loading1:false,\n      loading:false,\n      err:'',\n      qty:0,\n      unit:'',\n      food:'',\n      searchItem:'',\n      searchList:[] ,\n      post_data :{\n        \"ingr\":[]\n      },\n      post_response:{},\n      get_response:{}\n    }\n  },\n\n  methods: {\n    add() {\n      this.searchItem= this.qty +\" \"+ this.unit +\" \"+ this.food;\n      this.searchList.push(this.searchItem);\n    }, \n\n    deleteItem(foodItem) {\n      this.searchList=this.searchList.filter(function(value){\n        return !(value===foodItem);\n       }); \n    },\n\n    submit () {\n      this.post_data.ingr=this.searchList;\n      this.loading1=true;\n      Axios.post(\"https://api.edamam.com/api/nutrition-details?app_id=\"+this.app_id+\"&app_key=\"+this.app_key, this.post_data).then(response=> { \n             console.log('SUCCESS', response.data);\n             this.post_response=response.data;\n             this.loading1=false;\n\n             });\n    },\n\n    getFacts(ingr){\n       this.loading=true;\n            Axios.get(\"https://api.edamam.com/api/nutrition-data?app_id=\"+this.app_id+\"&app_key=\"+this.app_key + \"&nutrition-type=cooking&ingr=\"+ingr).then(response=> {\n                this.get_response=response.data;\n                this.loading=false;\n            }).catch(err=> {\n                this.loading=false;\n                this.err=\"An error has occurred. Please try again\";\n                console.log('Error',err);\n            });\n\n    },\n\n\n \n  }\n}\n\n/* var closebtns = document.getElementsByClassName(\"close\");\n      var i;\n\n/* Loop through the elements, and hide the parent, when clicked on \n      for (i = 0; i < closebtns.length; i++) {\n    closebtns[i].addEventListener(\"click\", function() {\n    this.parentElement.style.display=\"none\";\n  }); } */\n</script>\n\n<style >\n.inputs{\n  margin-left: 15px;\n  margin-right: 15px;\n  padding: 5px;\n}\nspan{\n  min-width:100px;\n}\n\n#pulser {\n      animation: buttonpulse 1s infinite;\n      border: 1px solid blue;\n      background-color: coral;\n  }\n\n  @keyframes buttonpulse {\n      0% {\n    box-shadow: 0 0 0 0px rgba(20, 180, 0, 0.2);\n  }\n  100% {\n    box-shadow: 0 0 0 10px rgba(20, 0, 160, 0.3);\n  }\n\n  }\n\nh2{\n  color:lightseagreen;\n  text-transform: capitalize;\n  text-decoration: underline;\n  \n}\np{\n  color: seagreen;\n}\n\nb{\n  color:coral;\n}\n\n.container {\n     justify-content: center;\n     margin-left: auto;\n     margin-right: auto;\n     display: inline-block;\n     /*margin-left:240px; */\n } \n\n.card {\ncolor: darkcyan;\nborder: 2px solid goldenrod;\npadding: 10px;\nwidth: 250px;\nmargin: 10px;\n}\n\ntable {\n    border: 1px solid slateblue;\n    display: inline-block;\n}\n\ntd {\n    border: 2px solid goldenrod;\n}\n\n tr:hover {\n  border: 3px solid gold;\n  background-color: aqua;\n  color:darkred;\n}\n\nth {\n    border: 3px solid royalblue;\n    background-color: aquamarine;    \n} \n\n .centred {\n     margin-left:auto; \n     margin-right:auto;\n }\n\n .panel{\n  \n   border: 2px solid springgreen;\n   background-color:powderblue;\n   color:darkorchid;\n   width: 220px;\n }\n\n</style>\n  "]}]}